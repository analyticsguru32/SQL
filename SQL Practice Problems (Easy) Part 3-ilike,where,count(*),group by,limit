1-
select actor_id 
from actor
where first_name ilike 'ed' and last_name ilike 'chase'

2-
select category_id,count(*) as film_count
from film_category
group by category_id
order by category_id desc
limit 1

3-with cte1 as(
select actor_id,count(*) as no_of_movies
from film_actor
group by actor_id
order by no_of_movies desc
limit 1
